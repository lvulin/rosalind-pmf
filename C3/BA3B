def string_spelled(genome_path):
  result=genome_path[0]
  for i in range(1,len(genome_path)):
    result=result+genome_path[i][-1]
    #print(i)
    #print (result)
    
  return result 
  
  x="""ACCGA
CCGAA
CGAAG
GAAGC
AAGCT"""
inlines=x.split()
print(inlines)

result=string_spelled(inlines)
print(result)
  
  
